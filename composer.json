{
    "name": "clumsy/notifier",
    "description": "Notifications for Laravel",
    "keywords": ["laravel", "cms", "notifications"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "clumsy/assets": "dev-master",
        "intervention/image": "2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Clumsy\\Notifier": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
